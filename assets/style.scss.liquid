/* CSS stykes go in here */

body {
    margin: 0;
    padding: 0;
    font-family: 'Open Sans', sans-serif;
}

header {
    background-color: #fff;
    padding: 15px 0;
}

header nav {
    padding: 5px 0;
    background-color: #222;
    text-transform: uppercase;
    font-size: 20px;
}

header nav li {
    margin: 0 15px;
    display: inline-block;
    vertical-align: top;
}

header nav a {
    color: #fff;
    text-decoration: none;
}